<p id="notice"><%= notice %></p>
<p>
  <strong>Name:</strong>
  <%= @feed.name %>
</p>

<p>
  <strong>Url:</strong>
  <%= @feed.rss %>
</p>

<p>
  <strong>Description:</strong>
  <%= @feed.description %>
</p>


<%= link_to 'Edit', edit_feed_path(@feed) %> |
<%= link_to 'Back', feeds_path %>
<%= paginate @entry %>
    <div id="wrapper">
      <div id="masonry-container">
        <% @entry.each do |r| %>
          <div class="mas-box"><%= image_tag r.image.mid.url if r.image?  %><%= r.title %></div>
        <% end %>
      </div>
    </div>

<%= paginate @entry %>
