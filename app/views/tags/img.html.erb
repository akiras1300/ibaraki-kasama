<div class="container">
  <%= @tag.name %>の画像追加
  <%= bootstrap_form_for(Img.new,url: {action: 'imgset'}, layout: :horizontal, label_col: "col-sm-2", control_col: "col-sm-8") do |f| %>
    <%= f.text_field :title, wrapper: { class: 'row' } %>
    <%= f.file_field :image, wrapper: { class: 'row' } %>
    <%= f.text_field :inyo, wrapper: { class: 'row' } %>
    <%= f.submit "完了",{ class: 'btn btn-primary' }%>
  <% end %>
</div>
