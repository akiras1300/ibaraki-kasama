<div class="container">
  <%= @tag.name %>のURL追加
  <%= bootstrap_form_for(Url.new,url: {action: 'urlset'}, layout: :horizontal, label_col: "col-sm-2", control_col: "col-sm-8") do |f| %>
    <%= f.text_field :title, wrapper: { class: 'row' } %>
    <%= f.text_field :url, wrapper: { class: 'row' } %>
    <%= f.file_field :image, wrapper: { class: 'row' } %>
    <%= f.submit "完了",{ class: 'btn btn-primary' }%>
  <% end %>
</div>
